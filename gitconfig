[core]
    autocrlf = false
    excludesfile = ~/.gitignore_global
[alias]
    lg = log --graph --pretty=format:'%Cred%h %d%Creset %s %Cblue(%ar by %an)'
    ll = log --name-status --color --graph
    branches = branch -a
    heads = show-ref --heads
    tags = show-ref --tags
    pll = pull --rebase
    pff = pull --ff-only
    co = checkout
