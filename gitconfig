[user]
    name = James H. Linder
    email = james@jlinder.com
[core]
    autocrlf = false
    excludesfile = ~/.gitignore_global
[alias]
    a = add
    b = branch
    ba = branch -a
    branches = branch -a
    c = commit -v
    co = checkout
    d = diff
    heads = show-ref --heads
    lg = log --graph --pretty=format:'%Cred%h %d%Creset %s %Cblue(%ar by %an)'
    ll = log --name-status --color --graph
    pff = pull --ff-only
    pll = pull --rebase
    remotes = remote -v
    s = status
    tags = show-ref --tags
